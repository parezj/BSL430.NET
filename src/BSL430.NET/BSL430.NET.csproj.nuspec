<?xml version="1.0"?>
<package >
    <metadata>
        <id>BSL430.NET</id>
        <version>1.3.0</version>
        <authors>Jakub Parez</authors>
        <owners>Jakub Parez</owners>
        <projectUrl>https://github.com/parezj/BSL430.NET</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/parezj/BSL430.NET/master/img/logo/logo2.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>TI MSP430 Bootloader (BSL) .NET Cross-Platform Toolchain. With generic UART converter can easily manipulate with MCU memory. Intel-HEX, TI-TXT, ELF, SREC support.</description>
        <releaseNotes>First public release of package. Win x86/amd64 tested. FTDI, Serial port (5xx protocol) tested.</releaseNotes>
        <copyright>Copyright (c) 2019 Jakub Parez</copyright>
        <license type="expression">MIT</license>
        <tags>MSP430 Texas Instruments MSP 430 firmware MCU BSL bootloader flash memory toolchain BSL430 BSL430.NET download upload erase FTDI FT232 Serial COM USB Intel HEX TI-TXT SREC ELF</tags>
        <dependencies>
            <dependency id="BSL430.NET.FirmwareTools" version="1.3.0" />
            <dependency id="HidSharp" version="2.1.0" />
            <dependency id="LibUsbDotNet" version="2.2.29" />
            <dependency id="SerialPortStream" version="2.2.0" />
            <dependency id="System.ValueTuple" version="4.5.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="bin\AnyCPU\Debug\netstandard2.0\BSL430.NET.dll" target="lib/netstandard2.0" />
        <file src="bin\AnyCPU\Debug\netstandard2.0\BSL430.NET.xml" target="lib/netstandard2.0" />
        <file src="bin\AnyCPU\Debug\net461\BSL430.NET.dll" target="lib/net461" />
        <file src="bin\AnyCPU\Debug\net461\BSL430.NET.xml" target="lib/net461" />

        <file src="lib\win-amd64\*.*" target="runtimes/win-x64/native/" />
        <file src="lib\win-x86\*.*" target="runtimes/win-x86/native/" />
        <file src="lib\linux-amd64\*.*" target="runtimes/linux-x64/native/" />

        <file src="BSL430.NET.targets" target="build" />
    </files>
</package>
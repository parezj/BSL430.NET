<Application x:Class="BSL430_NET_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BSL430_NET_WPF">
    
    <Application.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:AppBootstrapper x:Key="bootstrapper" />
                </ResourceDictionary>
                
                <ResourceDictionary Source="pack://application:,,,/BSL430.NET.WPF;component/Styles/MainGeneric.xaml" />
                <ResourceDictionary Source="pack://application:,,,/BSL430.NET.WPF;component/Styles/MainLight.xaml" />
                <ResourceDictionary Source="pack://application:,,,/BSL430.NET.WPF;component/Styles/ControlProcessStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/BSL430.NET.WPF;component/Styles/Controls.xaml" />
                <ResourceDictionary Source="pack://application:,,,/BSL430.NET.WPF;component/Styles/ContextMenu.xaml" />
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
